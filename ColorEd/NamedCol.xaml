        <HyperlinkButton x:Name="hyperlinkButton1" Content="Color Blindness" HorizontalAlignment="Left" Margin="769,20,0,0" Grid.Row="1" VerticalAlignment="Top"/>
        <Grid x:Name="grid" Margin="120,91,0,0" Grid.Row="1">
